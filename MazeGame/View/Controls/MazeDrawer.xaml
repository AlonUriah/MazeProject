<UserControl x:Class="MazeGame.View.Controls.MazeDrawer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MazeGame.View.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <DockPanel HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Grid Name="MazeGrid" Loaded="MazeGrid_Loaded" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" KeyDown="MazeGrid_KeyDown" />
    </DockPanel>
</UserControl>
