<Window x:Class="MazeGame.View.Multiplayer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MazeGame.View"
        mc:Ignorable="d"
        Title="Multiplayer" Height="300" Width="300">
    <DockPanel>
        <Button DockPanel.Dock="Top" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Back To Main Menu" />
        <Label DockPanel.Dock="Right" Content="Other Player's Board" FontSize="15" Foreground="Green" VerticalAlignment="Top" HorizontalAlignment="Center"/>
        <Grid DockPanel.Dock="Right" HorizontalAlignment="Center" VerticalAlignment="Center" Background="Black">
        </Grid>
        <Label DockPanel.Dock="Left" Content="My Board" FontSize="15" Foreground="Blue" VerticalAlignment="Top" HorizontalAlignment="Center"/>
        <Grid DockPanel.Dock="Left" HorizontalAlignment="Center" VerticalAlignment="Center">
        </Grid>
    </DockPanel>
</Window>
